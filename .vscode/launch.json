{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/dist/main.js",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "args": [
        "Images/source_images/feep.ppm",
        "Images/key_images/feep.ppm",
        "motionblur",
        "10"
      ],
      "skipFiles": ["<node_internals>/**"],
      "preLaunchTask": {
        "type": "typescript",
        "tsconfig": "tsconfig.json"
      }
    }
  ]
}
